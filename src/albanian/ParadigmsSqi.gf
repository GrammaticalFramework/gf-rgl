mkPConj : Str -> PConj = \s -> lin PConj {s=s} ;

mkPron : (nom,acc,dat,ablat,acc_clit,dat_clit : Str) -> GenNum -> Person -> Pron =
  \nom,acc,dat,ablat,acc_clit,dat_clit,gn,p -> lin Pron 
     {s = table Case [nom; acc; dat; ablat];
      acc_clit = acc_clit;
      dat_clit = dat_clit;
      a = {gn=gn; p=p}
     } ;

