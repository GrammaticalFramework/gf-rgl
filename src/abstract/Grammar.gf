--1 Grammar: the Main Module of the Resource Grammar

-- This grammar is a collection of the different grammar modules,
-- To test the resource, import [``Lang`` Lang.html], which also contains
-- a lexicon.

abstract Grammar = 
  Noun,
  Verb, 
  Adjective,
  Adverb,
  Numeral,
  Sentence, 
  Question,
  Relative,
  Conjunction,
  Phrase,
  Text,
  Structural,
  Idiom,
  Tense,
  Transfer 
  ;

