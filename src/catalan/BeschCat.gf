oper deaccent : Str -> Str = \témer ->
  case témer of {
    t@_ + "à" + mer@_ => t + "a" + mer ;
    t@_ + "é" + mer@_ => t + "e" + mer ;
    t@_ + "è" + mer@_ => t + "e" + mer ;
    t@_ + "í" + mer@_ => t + "i" + mer ;
    t@_ + "ó" + mer@_ => t + "o" + mer ;
    t@_ + "ò" + mer@_ => t + "o" + mer ;
    t@_ + "ú" + mer@_ => t + "u" + mer ;
    _                 => témer 
  } ;

oper soften : Str -> Str = \venc ->
  case venc of {
    ven + "c" => ven + "ç" ;
    men + "g" => men + "j" ;
    _         => venc  
  } ;
